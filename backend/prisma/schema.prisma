generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Category {
  id        String   @id @default(uuid())
  name      String
  amount    Float
  icon      String
  createdBy DateTime

  Expense Expense[]
}

model Expense {
  id        String   @id @default(uuid())
  name      String
  amount    Float
  createdAt DateTime

  category   Category @relation(fields: [categoryId], references: [id])
  categoryId String
}
